= form_with model: project, local: true do |f|
  = f.label :title
  %br
  = f.text_field :title
  %br
  = f.label "タグ  ※以下、自由にタグを入力してください"
  %br
  %ul#project-tags
  = hidden_field_tag :category_hidden, @category_list
  %br
  = f.label :content
  %br
  = f.text_area :content
  %br
  = f.label :goal_amount
  %br
  = f.text_field :goal_amount
  %br
  = f.label :limit
  %br
  = f.date_select :limit
  %br
  = f.submit